    {% else %}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/leaflet.js" defer async></script>
        <script src="https://unpkg.com/tangram/dist/tangram.min.js" defer async></script>

        <script src="https://api.tourism-system.com/render/assets/scripts/raccourci/playlist-map.leaflet.debug.js?otmKey=IVvN8vTvQ9OC94ltxJGaKw&gmKey=AIzaSyCfQ5H4V8Q7YqtgxaeHxhIqdq42x5QbyEs" defer async></script>
