{% endblock metas %}

{% block content %}
    <main class="content-type-page" ng-app="{{ ng_app|default('drupalAngularApp') }}">
        <div class="content-wrapper">
            {# {% if page_hero | default %}
                {{ page_hero }}
            {% endif %} #}
            {# L'en-tête de page est toujours affichée même si aucun champ n'est saisi afin d'afficher le fil d'arianne #}
