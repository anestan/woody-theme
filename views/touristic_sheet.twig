{% extends "base.twig" %}

{% block metas %}
    {% for meta in metas %}
        {{ meta }}
    {% endfor %}
{% endblock metas %}

{% block content %}

    <main class="content-type-page">
        <div class="content-wrapper">
            {% if sheet_template | default %}{{ sheet_template }}{% endif %}
        </div>
    </main>
{% endblock content %}

{% block styles %}
    {# <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css"> #}
    <link rel="stylesheet" href="https://api.tourism-system.com/render/assets/angular/styles/vendor.css">

    {# <link rel="stylesheet" href="https://api.tourism-system.com/render/assets/styles/sheet.css"> #}
{% endblock styles %}

{% block inline_scripts %}

    <!-- DrupalAngularConfig generated by Wordpress  -->
    <script type="text/javascript">
        DrupalAngularConfig.fetcherType = "{{ fetcherType|default }}" || null;
        DrupalAngularConfig.playlistId = "{{ playlistId|default }}" || null;
        DrupalAngularConfig.destinationName = "{{ destinationName|default }}" || null;
        DrupalAngularConfig.lang = "{{ lang|default }}" || null;
        {% if timezone is defined %}
        DrupalAngularConfig.timezone = "{{ timezone|default }}" || null;
        {% endif %}
        {% if season is defined %}
        DrupalAngularConfig.season = "{{ season|default }}" || null;
        {% endif %}
    </script>

    <!--
        <script src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
        <script src="https://api.tourism-system.com/render/assets/scripts/raccourci/sheet_item.min.js"></script>
        <script type="text/javascript" src="www.google-analytics.com/ga.js"></script>
    -->
    <!--
            {% if object.data.bordereau in ['PRD','ITI'] and global.request.attributes is defined and global.request.attributes.get('part') == 'preview' %}
        <script src="{{ global.request.basepath }}/assets/scripts/raccourci/itinerary.js"></script>
        {% endif %}
    -->

{% endblock inline_scripts %}
